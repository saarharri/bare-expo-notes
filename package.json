{
  "private": false,
  "name": "bare-expo-notes",
  "description": "Example of embedding Bare in an Expo application with sharing notes feature.",
  "main": "expo-router/entry",
  "scripts": {
    "build-pear-end": "npx bare-pack --target ios --target android  --linked --out app/app.bundle.mjs backend/backend.mjs",
    "dev:android": "npm run build-pear-end && npm run android",
    "start": "expo start",
    "android": "expo run:android",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saarharri/bare-expo-notes.git"
  },
  "author": "saarharri",
  "license": "Apache-2.0",
  "homepage": "https://github.com/saarharri/bare-expo-notes#readme",
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@reduxjs/toolkit": "^2.2.7",
    "autopass": "^2.2.1",
    "b4a": "^1.6.7",
    "bare-fs": "^4.2.1",
    "bare-rpc": "^0.2.8",
    "corestore": "^7.4.5",
    "events": "^3.3.0",
    "expo": "^53.0.9",
    "expo-build-properties": "^0.14.6",
    "expo-constants": "^17.1.6",
    "expo-linking": "^7.1.5",
    "expo-router": "^5.0.6",
    "expo-system-ui": "^5.0.7",
    "hypercore-crypto": "^3.6.1",
    "hyperswarm": "^4.13.1",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-native": "^0.79.2",
    "react-native-bare-kit": "^0.8.1",
    "react-native-safe-area-context": "^5.4.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@types/b4a": "^1.6.5",
    "@types/react": "^19.1.3",
    "bare-pack": "^1.4.4",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0",
    "typescript": "^5.3.3"
  }
}
